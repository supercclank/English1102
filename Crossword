<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js">
</script>
<script>
var solution = "abcdefgh";
var totalLetters = 8;
$(document).ready(
new function(){
	$('td.empty').append("<input type='text' maxlength='1' class='cell'></input>");
	alert("derp");
}
	);

function check(){
	var guess=[];
	var numCorrect = 0;
	$('.cell').each(function(){guess.push($(this).val());});
	for (var x =0; x <guess.length; x++){
		if(guess[x] === solution[x]){
			numCorrect++;
		}
	}
	if(numCorrect == totalLetters){
		alert("puzzle complete");
	}
}
</script>
</html>
